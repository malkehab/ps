#include<bits/stdc++.h>
using namespace std;
int main(){
 
    int n1 , n2 , n3 ;
    cin >> n1 >> n2 >> n3 ;
 
    if ( n1<=n2 && n2<=n3){
        cout << n1 << "\n" << n2 << "\n" << n3 << "\n\n";
    }
    else if ( n1<=n3 && n3<=n2){
        cout << n1 << "\n" << n3 << "\n" << n2 << "\n\n";
    }
    else if ( n2<=n1 && n1<=n3){
        cout << n2 << "\n" << n1 << "\n" << n3 << "\n\n";
    }
    else if ( n2<=n3 && n3<=n1){
        cout << n2 << "\n" << n3 << "\n" << n1 << "\n\n";
    }
    else if ( n3<=n1 && n1<=n2){
        cout << n3 << "\n" << n1 << "\n" << n2 << "\n\n";
    }
    else{
        cout << n3 << "\n" << n2 << "\n" << n1 << "\n\n";
    }
 
    cout << n1 << "\n" << n2 << "\n" << n3 << endl ;
 
    return 0 ;
}
