#include<iostream>
using namespace std ;
int main(){
 
    string a1, b1 ; 
    cin >> a1 ;
 
    for ( int i =a1.size()-1; i>=0; i-- ){
        b1 += a1[i] ;
    }
 
    int a2, b2 ;
    a2 = stoi(a1) ;
    b2 = stoi(b1) ;
    cout << b2 << "\n" ;
 
    if (a2==b2){
        cout << "YES\n" ;
    }
    else{
        cout << "NO\n" ;
    }
 
    return 0 ;
    
}
