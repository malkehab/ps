#include<iostream>
using namespace std ; 
int main(){
    
    int n1 , n2 , sum;
    while(true){
        sum = 0 ;
        cin >> n1 >> n2 ;
        if (n1<=0 || n2<=0){
            break ;
        }
        else if (n1>n2){
            for (int i=n2; i<=n1; i++ ){
                cout << i << " " ;
                sum += i ;
            }
            cout << "sum ="<< sum << "\n" ;
        }
        else {
            for (int i=n1; i<=n2; i++ ){
                cout << i << " " ;
                sum += i ;
            }
            cout << "sum ="<< sum << "\n" ;
        }
    }
    
    return 0 ;
}
